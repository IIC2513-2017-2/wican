{
  "presets": [
    [
      "env",
      {
        "targets": {
          "ie": 9,
          "browsers": [">1%", "last 3 versions"]
        },
        "modules": false,
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-rest-spread",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
